#include<stdio.h>
int partition(int *arr,int first,int end){
        int pi =first;
        int start=first;
        int finish=end;
        int pivot = arr[pi],temp;
        while(start<finish){
            while(pivot>=arr[start]){
                start++;
            }
            while(pivot<arr[finish]){
                finish--;
            }
            if(start<finish){
                temp=arr[start];
                arr[start]=arr[finish];
                arr[finish]=temp;
            }
        }
        temp=arr[pi];
        arr[pi]=arr[finish];
        arr[finish]=temp;
        return finish;
}
void quicksort(int *arr,int first,int end){
    int pi;
    if(first<end){
        pi=partition(arr,first,end);
        quicksort(arr,first,pi-1);
        quicksort(arr,pi+1,end);
    }
}
int main(){
    int arr[10]={3,5,1,7,6,8,2,4,9,10};
    quicksort(arr,0,9);
    for(int i=0;i<10;i++){
        printf("%d\t",arr[i]);
    }
    return 0;
}
